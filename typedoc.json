{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/stats/src/index.ts",
    "packages/crdt/src/index.ts"
  ],
  "out": "docs/api",
  "entryPointStrategy": "packages",
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "README.md",
  "plugin": [],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "T-Tests",
    "Regression",
    "Utilities",
    "Counters",
    "Sets",
    "*"
  ],
  "sort": ["source-order"],
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": false
  }
}
